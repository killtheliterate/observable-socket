{
  "name": "observable-socket",
  "version": "5.0.3",
  "description": "Wraps a websocket in an Observable.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "npm run build:a && npm run build:b",
    "build:a": "./node_modules/.bin/cross-env NODE_ENV=production node build/index.js",
    "build:b": "./node_modules/.bin/cross-env NODE_ENV=production BABEL_ENV=observableonly node build/index.js",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "standard && mocha __test && mocha __test/spec/bind.test.js"
  },
  "standard": {
    "globals": [
      "WebSocket",
      "afterEach",
      "beforeEach",
      "describe",
      "it",
      "xit"
    ]
  },
  "keywords": [
    "observable",
    "rx",
    "rxjs",
    "socket",
    "websocket",
    "ws"
  ],
  "bugs": {
    "url": "https://github.com/killtheliterate/observable-socket/issues"
  },
  "contributors": [
    "Garrett Dawson <killtheliterate@gmail.com> (http://thedrearlight.com/)",
    "Talon Poole <talon@theghostin.me> (http://theghostin.me)"
  ],
  "homepage": "https://github.com/killtheliterate/observable-socket",
  "repository": {
    "type": "git",
    "url": "https://github.com/killtheliterate/observable-socket.git"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^3.1.0",
    "rxjs": "^5.4.1"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.0.1",
    "mocha": "^4.0.1",
    "mock-socket": "^7.1.0",
    "rollup": "^0.51.7",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "standard": "^10.0.3",
    "ws": "^3.3.1"
  }
}
