{
  "name": "observable-socket",
  "version": "2.1.0",
  "description": "Wraps a websocket in an Observable.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && ./node_modules/.bin/babel src --out-dir dist",
    "lint": "eslint src/** test/**",
    "lint:quiet": "eslint --quiet src/** test/**",
    "prepublish": "rm -rf ./dist && ./node_modules/.bin/babel src --out-dir dist",
    "pretest": "rm -rf ./dist && ./node_modules/.bin/babel src --out-dir dist;",
    "test": "npm run lint:quiet && ./node_modules/.bin/mocha test/node && node test/browser/bootstrap & ./node_modules/.bin/karma start karma.conf.js --browsers Firefox --single-run",
    "test:browser": "node test/browser/bootstrap & npm run pretest && ./node_modules/.bin/karma start karma.conf.js --single-run",
    "test:node": "npm run pretest && ./node_modules/.bin/mocha test/node"
  },
  "keywords": [
    "socket",
    "observable",
    "ws",
    "websocket"
  ],
  "bugs": {
    "url": "https://github.com/killtheliterate/observable-socket/issues"
  },
  "contributors": [
    "Garrett Dawson <killtheliterate@gmail.com> (http://thedrearlight.com/)",
    "Talon Poole <talon@theghostin.me> (http://theghostin.me)"
  ],
  "homepage": "https://github.com/killtheliterate/observable-socket",
  "repository": {
    "type": "git",
    "url": "https://github.com/killtheliterate/observable-socket.git"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "lodash": "^3.10.1",
    "rxjs": "^5.0.0-beta.6"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "karma": "^0.13.16",
    "karma-browserify": "^4.4.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-opera-launcher": "^0.3.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-spec-reporter": "0.0.23",
    "mocha": "^2.3.4",
    "watch": "^0.17.1",
    "ws": "^0.8.1"
  }
}
